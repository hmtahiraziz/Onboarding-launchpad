{
  "name": "paramount-launchpad-backend",
  "version": "1.0.0",
  "description": "Backend API for Paramount Launchpad - AI-powered liquor wholesale onboarding",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:all": "jest --testPathPattern=tests",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:watch:unit": "jest --testPathPattern=tests/unit --watch",
    "test:watch:integration": "jest --testPathPattern=tests/integration --watch",
    "test:watch:e2e": "jest --testPathPattern=tests/e2e --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "test:clean": "rimraf coverage dist",
    "test:report": "jest --coverage && open coverage/lcov-report/index.html",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "node-version": "node --version",
    "test:node21": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest",
    "test:node21:coverage": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest --coverage",
    "sync:products": "ts-node src/scripts/sync-products-advanced.ts",
    "migrate": "ts-node src/scripts/run-migrations.ts",
    "test:migration": "ts-node src/scripts/test-migration.ts"
  },
  "keywords": [
    "liquor",
    "wholesale",
    "ai",
    "onboarding",
    "typescript"
  ],
  "author": "Paramount Launchpad Team",
  "license": "MIT",
  "engines": {
    "node": ">=21.7.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^5.20.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "testcontainers": "^10.7.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
